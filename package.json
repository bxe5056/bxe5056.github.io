      "^svgo/dist/svgo.browser$": "<rootDir>/src/__mocks__/svgo.js",
      "^html-table-to-json$": "<rootDir>/src/__mocks__/html-table-to-json.js",
      "^markdown-table$": "<rootDir>/src/__mocks__/markdown-table.js",
      "^framer-motion$": "<rootDir>/src/__mocks__/framer-motion.js"
    },
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "presets": [
            "react-app"
          ]
        }
      ]
    }
  }
